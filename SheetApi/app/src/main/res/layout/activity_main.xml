<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity">


    <TextView
        android:id="@+id/tv1"
        android:textSize="20sp"
        android:textAllCaps="true"
        android:padding="8dp"
        android:paddingLeft="4dp"
        android:textColor="@color/colorPrimaryDark"
        android:text="@string/Sr_No"
        android:layout_width="82dp"
        android:layout_height="47dp"
        android:layout_margin="8dp"
        android:layout_marginLeft="12dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/et5"
        android:textSize="20sp"
        android:padding="8dp"
        android:paddingLeft="4dp"
        android:textAllCaps="true"
        android:textColor="@color/colorPrimaryDark"
        android:text="@string/key"
        android:layout_width="82dp"
        android:layout_height="47dp"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintStart_toEndOf="@id/et1"
        android:layout_margin="8dp"/>

    <EditText
        android:id="@+id/et3"
        android:layout_width="82dp"
        android:layout_height="47dp"
        android:layout_margin="8dp"
        app:layout_constraintStart_toEndOf="@id/et2"
        app:layout_constraintTop_toTopOf="parent" />

    <EditText
        android:id="@+id/et4"
        android:layout_width="82dp"
        android:layout_height="47dp"
        android:layout_margin="8dp"
        app:layout_constraintStart_toEndOf="@id/et3"
        app:layout_constraintTop_toTopOf="parent" />

    <EditText
        android:id="@+id/et2"
        android:layout_width="82dp"
        android:layout_height="47dp"
        android:layout_margin="8dp"
        app:layout_constraintStart_toEndOf="@id/et1"
        app:layout_constraintTop_toTopOf="parent"


         />

    <EditText
        android:id="@+id/et6"
        android:layout_width="82dp"
        android:layout_height="47dp"
        app:layout_constraintTop_toBottomOf="@id/et2"
        app:layout_constraintEnd_toStartOf="@id/et3"
        android:layout_margin="8dp"

        />

    <EditText
        android:id="@+id/et7"
        android:layout_width="82dp"
        android:layout_height="47dp"
        app:layout_constraintTop_toBottomOf="@id/et3"
        app:layout_constraintEnd_toStartOf="@id/et4"
        android:layout_margin="8dp"

        />

    <EditText
        android:id="@+id/et8"
        android:layout_width="82dp"
        android:layout_height="47dp"
        app:layout_constraintTop_toBottomOf="@id/et3"
        app:layout_constraintLeft_toRightOf="@id/et7"
        android:layout_margin="8dp"

        />

    <TextView
        android:id="@+id/et9"
        android:textAllCaps="true"
        android:padding="8dp"
        android:paddingLeft="4dp"
        android:text="@string/value"
        android:textColor="@color/colorPrimaryDark"
        android:textSize="20sp"
        android:layout_width="82dp"
        android:layout_height="47dp"
        app:layout_constraintTop_toBottomOf="@id/et5"
        app:layout_constraintEnd_toStartOf="@id/et2"
        android:layout_margin="8dp"

        />

    <EditText
        android:id="@+id/et10"
        android:layout_width="82dp"
        android:layout_height="47dp"
        app:layout_constraintTop_toBottomOf="@id/et6"
        app:layout_constraintEnd_toStartOf="@id/et3"
        android:layout_margin="8dp"

        />

    <EditText
        android:id="@+id/et11"
        android:layout_width="82dp"
        android:layout_height="47dp"
        app:layout_constraintTop_toBottomOf="@id/et7"
        app:layout_constraintEnd_toStartOf="@id/et4"
        android:layout_margin="8dp"

        />

    <EditText
        android:id="@+id/et12"
        android:layout_width="82dp"
        android:layout_height="47dp"
        app:layout_constraintTop_toBottomOf="@id/et8"
        app:layout_constraintLeft_toRightOf="@id/et7"
        android:layout_margin="8dp"

        />

    <Button
        android:id="@+id/bt1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        android:layout_margin="8dp"

        />






</androidx.constraintlayout.widget.ConstraintLayout>